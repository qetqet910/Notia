{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-hide",
    "core:window:allow-show",
    "process:allow-exit",
    "log:default",
    "notification:default",
    "sql:default",
    "sql:allow-execute",
    "sql:allow-select",
    "updater:default"
  ]
}
